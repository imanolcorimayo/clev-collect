{
  "functions": {
    "source": "functions",
    "predeploy": [
      "npm run build", "./predeploy-functions.bat"
    ]
  },
  "hosting": {
    "predeploy": [
      "npm run build", "./predeploy-hosting.bat"
    ],
    "public": "public",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
    {
      "source": "**",
      "function": "ssrapp"
    }
  ]
  }
}